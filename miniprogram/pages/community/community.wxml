<van-tabbar class="tarbar" active-color="blue" style="z-index: 99999;" active-color="#07c160" inactive-color="#000" active="{{ active }}" bind:change="onChange">
    <van-tabbar-item icon="fire">Current</van-tabbar-item>
    <van-tabbar-item icon="todo-list">Projects</van-tabbar-item>
    <van-tabbar-item icon="friends">Friends</van-tabbar-item>
    <van-tabbar-item icon="setting">Settings</van-tabbar-item>
</van-tabbar>
<view style="width: 100vw;height: 100vh;overflow: hidden;">
    <scroll-view style="height: 90vh;width: 100vw;" scroll-y="true">
        <view wx:for="{{catcatarr}}" wx:key="catcat" class="card">
            <van-circle class="circlet" value="{{ item.catcatpro }}" stroke-width="8" layer-color="white" color="#577df9" />
            <text>{{item.catcat}}</text>
        </view>
    </scroll-view>


    <view style="height:5rem;width:100%"></view>
</view>