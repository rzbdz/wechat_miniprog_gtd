<view style="min-height: 100vh;width: 100vw; overflow: hidden;">
    <scroll-view class = "sv" placeholder="true" scroll-y="true">
        <view wx:for="{{catcatarr}}" wx:key="catcat" class="card">
            <van-circle class="circlet" value="{{ item.catcatpro }}" stroke-width="8" layer-color="white" color="#ab0005" />
            <text>{{item.catcat}}</text>
        </view>
    </scroll-view>
    <van-tabbar active-color="#ab0005" inactive-color="#999999" active="{{ 1 }}" bind:change="onChange">
        <van-tabbar-item icon="fire">现在</van-tabbar-item>
        <van-tabbar-item icon="todo-list">死线</van-tabbar-item>
        <van-tabbar-item icon="friends">外包</van-tabbar-item>
        <van-tabbar-item icon="setting">设置</van-tabbar-item>
    </van-tabbar>
</view>